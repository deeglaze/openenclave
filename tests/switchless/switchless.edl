enclave {
    trusted {
        public int enc_echo_switchless(
            [string, in] char* in,
            [out] char out[100]);
        public int enc_echo_regular(
            [string, in] char* in,
            [out] char out[100]);
    };

    untrusted {
        int host_echo_switchless(
            [string, in] char* in,
